{
  "private": false,
  "name": "astro-robots",
  "description": "The lightweight robots.txt generator makes it simple to create compliant robots.txt files specifically for Astro integrations, with zero-config support, an intuitive JSDoc API, and always up-to-date verified bots user-agents.",
  "version": "2.0.0",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/consts.d.ts"
  ],
  "scripts": {
    "clear": "rm -rf ./dist",
    "build": "at-scripts create-pkg-name && at-scripts build && tsc -p tsconfig.build.json"
  },
  "keywords": [
    "withastro",
    "astro-integration",
    "robots.txt generator",
    "seo assistant",
    "verified bots",
    "verified robots",
    "robots",
    "robots-txt",
    "astro-robots"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ACP-CODE/astro-robots.git"
  },
  "author": "Junlin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ACP-CODE/astro-robots/issues"
  },
  "homepage": "https://github.com/ACP-CODE/astro-robots#readme",
  "devDependencies": {
    "@types/node": "^20.10.4",
    "astro": "^4.0.3",
    "typescript": "^5.3.3"
  }
}
